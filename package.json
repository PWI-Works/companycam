{
  "name": "@companycam/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for the CompanyCam API generated from the published OpenAPI specification.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "node --version",
    "release": "semantic-release",
    "prepare": "node ./scripts/setup-hooks.mjs",
    "commit": "commit"
  },
  "keywords": [
    "companycam",
    "sdk",
    "typescript",
    "api"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PWI-Works/companycam.git"
  },
  "bugs": {
    "url": "https://github.com/PWI-Works/companycam/issues"
  },
  "homepage": "https://github.com/PWI-Works/companycam#readme",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@commitlint/prompt-cli": "20.1.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^10.0.0",
    "@semantic-release/npm": "^11.0.0",
    "husky": "^9.0.0",
    "rimraf": "^5.0.0",
    "semantic-release": "^24.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "axios-retry": "^4.5.0"
  }
}
